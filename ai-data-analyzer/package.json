{
  "name": "ai-data-analyzer",
  "version": "1.0.0",
  "description": "AI-powered data analysis tool with Master Agent coordination and MCP Server integration",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"node mcp-server.js\" \"electron . --dev\"",
    "server": "node mcp-server.js",
    "build": "electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "ai",
    "data-analysis",
    "claude",
    "electron",
    "mcp",
    "master-agent",
    "dashboard"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "fs-extra": "^11.3.0",
    "lru-cache": "^10.4.3",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2"
  },
  "build": {
    "appId": "com.yourcompany.ai-data-analyzer",
    "productName": "AI Master Agent Dashboard",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "index.html",
      "mcp-server.js",
      "assets/**/*",
      "uploads/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "uploads",
        "to": "uploads"
      }
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  }
}
